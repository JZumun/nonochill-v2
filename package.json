{
  "name": "nonochill",
  "version": "1.0.0",
  "private": true,
  "description": "Nonogram puzzle game implemented in Vue",
  "author": "Rein Bulaong",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@jzumun/nonogram-solver": "npm:@jsr/jzumun__nonogram-solver@^0.1.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "axios": "^1.8.4",
    "dotenv": "^5.0.1",
    "driver.js": "0.9",
    "eslint": "^8",
    "eslint-plugin-vue": "^8",
    "pretty": "^2.0.0",
    "pug": "^3.0.3",
    "stylus": "^0.54.5",
    "svg-inline-loader": "^0.8.0",
    "throttle-debounce": "^1.0.1",
    "v-hotkey3": "^0.1.4",
    "vite": "^6.0.6",
    "vite-svg-loader": "^5.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "MIT"
}
